        <section class="order-hist-main">
            <div class="page-header">
                <h1 class="order-page-title">My Orders</h1>
                <div class="filter-wrapper">
                    <button class="filter-button selected" id="current-orders-btn">Current Orders</button>
                    <button class="filter-button" id="order-history-btn">Order History</button>
                </div>
            </div>

            <div class="">
                <div class="content-block" id="current-orders-section">
                    <div class="block-heading">
                        <h2 class="block-name">ðŸšš Current Orders</h2>
                        <span class="count-pill count-pill-dark">
                            <%= currentOrders.length %> orders
                        </span>
                    </div>
                    <% if (currentOrders.length===0) { %>
                        <!-- Empty State -->
                        <div class="empty-orders-state">
                            <div class="empty-orders-image"></div>
                            <h3 class="empty-orders-title">No Orders Yet</h3>
                            <p class="empty-orders-text">
                                You haven't placed any orders yet. Start shopping to see your orders here!
                            </p>
                            <a href="/shop" class="shop-now-button">Start Shopping</a>
                        </div>
                        <% } else { %>
                            <div>
                                <% currentOrders.forEach(order=> { %>
                                    <div class="order-item">
                                        <div class="order-top">
                                            <span class="order-number">#<%= order._id.toString().slice(-5) %></span>
                                            <span class="order-phase">
                                                <%= order.status %>
                                            </span>
                                        </div>
                                        <div class="order-info">
                                            <div class="order-info-grid">
                                                <div class="order-info-item">
                                                    <div class="order-date">
                                                        Order Date: <%= new Date(order.createdAt).toLocaleDateString() %>
                                                    </div>
                                                </div>
                                                <div class="order-info-item">
                                                    <div class="order-amount">Total: <span>â‚¹<%= order.totalAmount %></span></div>
                                                </div>
                                                <div class="order-info-item">
                                                    <div class="order-payment-method">Payment Method: <span>
                                                        <%= order.paymentMethod==='COD' ? 'Cash on Delivery' : 'Paid Online' %>
                                                    </span></div>
                                                </div>
                                                <div class="order-info-item full-width">
                                                    <div class="order-address">Delivery Address: <span>
                                                        <%= order.deliveryAddress.address %>, <%= order.deliveryAddress.city %>, 
                                                        <%= order.deliveryAddress.state %>, <%= order.deliveryAddress.zipCode %>, 
                                                        <%= order.deliveryAddress.country %>
                                                    </span></div>
                                                </div>
                                                <div class="order-info-item full-width">
                                                    <div class="estimated-delivery">
                                                        Estimated Delivery: <span>
                                                            <% if (order.delivery_date) { %>
                                                                <%= new Date(order.delivery_date).toLocaleDateString('en-US', {
                                                                    year: 'numeric', month: 'long', day: 'numeric' 
                                                                }) %>
                                                            <% } else { %>
                                                                Not set
                                                            <% } %>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="delivery-tracker">
                                            <div class="tracker-stages">
                                                <div class="tracker-dot <%= order.status === 'Pending' || order.status === 'Processing' || order.status === 'Shipped' || order.status === 'Delivered' ? 'tracker-dot-complete' : '' %>"></div>
                                                <div class="tracker-dot <%= order.status === 'Processing' || order.status === 'Shipped' || order.status === 'Delivered' ? 'tracker-dot-complete' : '' %>"></div>
                                                <div class="tracker-dot <%= order.status === 'Shipped' || order.status === 'Delivered' ? 'tracker-dot-complete' : '' %>"></div>
                                                <div class="tracker-dot <%= order.status === 'Delivered' ? 'tracker-dot-complete' : '' %>"></div>
                                            </div>
                                            <div class="tracker-labels">
                                                <span class="tracker-label <%= order.status === 'Pending' ? 'tracker-label-active' : '' %>">Pending</span>
                                                <span class="tracker-label <%= order.status === 'Processing' ? 'tracker-label-active' : '' %>">Processing</span>
                                                <span class="tracker-label <%= order.status === 'Shipped' ? 'tracker-label-active' : '' %>">Shipped</span>
                                                <span class="tracker-label <%= order.status === 'Delivered' ? 'tracker-label-active' : '' %>">Delivered</span>
                                            </div>
                                        </div>
                                        <div class="order-products">
                                            <% order.items.forEach(item=> { %>
                                                <div class="product-details">
                                                    <div class="product-icon">
                                                        <img src="<%= item.product.images[0] %>" alt="<%= item.product.title %>">
                                                    </div>
                                                    <div class="product-info">
                                                        <div class="product-title">
                                                            <%= item.product.title %>
                                                        </div>
                                                        <div class="product-delivery-date">
                                                            Delivery: <% if (order.delivery_date) { %>
                                                                <%= new Date(order.delivery_date).toLocaleDateString('en-US', {
                                                                    year: 'numeric', month: 'long', day: 'numeric' 
                                                                }) %>
                                                            <% } else { %>
                                                                Not set
                                                            <% } %>
                                                        </div>
                                                    </div>
                                                </div>
                                            <% }) %>
                                            <% if (order.items.length > 2) { %>
                                                <div class="product-icon more-products">+<%= order.items.length - 2 %></div>
                                            <% } %>
                                        </div>
                                        <div class="order-controls">
                                            <button class="action-button button-secondary">Track Order</button>
                                            <a href="/product-details/<%= order.items[0].product._id %>">
                                                <button class="action-button button-primary">View Details</button>
                                            </a>
                                        </div>
                                    </div>
                                <% }) %>
                            </div>

                            <div class="stats-panel">
                                <div class="stats-heading">Order Summary</div>
                                <div class="stats-row">
                                    <span>Total Current Orders:</span>
                                    <span><%= currentOrders.length %></span>
                                </div>
                                <div class="stats-row">
                                    <span>Estimated Delivery:</span>
                                    <span id="delivery-date">
                                        <% if (currentOrders[0]?.delivery_date) { %>
                                            <%= new Date(currentOrders[0].delivery_date).toLocaleDateString('en-US', {
                                                year: 'numeric', month: 'long', day: 'numeric' 
                                            }) %>
                                        <% } else { %>
                                            Not set
                                        <% } %>
                                    </span>
                                </div>
                                <div class="stats-row">
                                    <span>Total Amount:</span>
                                    <span>â‚¹<%= currentOrders.reduce((total, order)=> total + order.totalAmount, 0) %></span>
                                </div>
                            </div>
                            <% } %>
                </div>

                <div class="content-block" id="order-history-section" style="display: none;">
                    <div class="block-heading">
                        <h2 class="block-name">ðŸ“¦ Order History</h2>
                        <span class="count-pill count-pill-medium">
                            <%= completedOrders.length %> orders
                        </span>
                    </div>
                    <% if (completedOrders.length===0) { %>
                        <div class="empty-history-state">
                            <div class="empty-history-image"></div>
                            <h3 class="empty-history-title">No Order History</h3>
                            <p class="empty-history-text">
                                You haven't completed any orders yet. Start shopping to build your order history!
                            </p>
                            <a href="/shop" class="history-shop-button">Browse Products</a>
                        </div>
                        <% } else { %>
                            <div class="">
                                <% completedOrders.forEach(order=> { %>
                                    <div class="order-item">
                                        <div class="order-top">
                                            <span class="order-number">#<%= order._id.toString().slice(-5) %></span>
                                            <span class="order-phase phase-complete">Delivered</span>
                                        </div>
                                        <div class="order-info">
                                            <div class="order-date">
                                                Order Date: <%= new Date(order.createdAt).toLocaleDateString() %>
                                            </div>
                                            <div class="order-delivery-date">
                                                Delivered On: <%= new Date(order.delivery_date).toLocaleDateString('en-US', {
                                                    year: 'numeric', month: 'long', day: 'numeric'
                                                }) %>
                                            </div>
                                            <div class="order-amount">Total: <span>â‚¹<%= order.totalAmount %></span>
                                            </div>
                                            <div class="order-payment-method">Payment Method: <span>
                                                    <%= order.paymentMethod==='COD' ? 'Cash on Delivery' : 'Paid Online'
                                                        %>
                                                </span></div>
                                            <div class="order-address">Delivery Address: <span>
                                                    <%= order.deliveryAddress.address %>, <%= order.deliveryAddress.city
                                                            %>, <%= order.deliveryAddress.state %>, <%=
                                                                    order.deliveryAddress.zipCode %>, <%=
                                                                        order.deliveryAddress.country %>
                                                </span></div>
                                        </div>
                                        <div class="order-products">
                                            <% order.items.forEach(item=> { %>
                                                <div class="product-details">
                                                    <div class="product-icon"><img src="<%= item.product.images[0] %>"
                                                            alt="<%= item.product.title %>"></div>
                                                    <div class="product-info">
                                                        <div class="product-title">
                                                            <%= item.product.title %>
                                                        </div>
                                                                                                    
                                                    </div>
                                                </div>
                                                <% }) %>
                                        </div>
                                        <div class="order-controls">
                                            <a href="/shop"><button class="action-button button-secondary">Buy
                                                    Again</button></a>
                                            <a href="/product-details/<%= order._id %>"> <button
                                                    class="action-button button-secondary">View Details</button></a>
                                        </div>
                                    </div>
                                    <% }) %>
                            </div>
                            <% } %>
                </div>
            </div>
        </section>
        <script>
            document.getElementById('current-orders-btn').addEventListener('click', function () {
                document.getElementById('current-orders-section').style.display = 'block';
                document.getElementById('order-history-section').style.display = 'none';
                this.classList.add('selected');
                document.getElementById('order-history-btn').classList.remove('selected');
            });

            document.getElementById('order-history-btn').addEventListener('click', function () {
                document.getElementById('current-orders-section').style.display = 'none';
                document.getElementById('order-history-section').style.display = 'block';
                this.classList.add('selected');
                document.getElementById('current-orders-btn').classList.remove('selected');
            });
        </script>


