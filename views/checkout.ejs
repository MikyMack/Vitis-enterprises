                <script>
                    async function handleCheckout(event) {
                        event.preventDefault();

                        const form = event.target;
                        const formData = new FormData(form);
                        const data = {
                            userId: formData.get('userId'),
                            items: JSON.parse(formData.get('cartItems')).map(item => ({
                                product: item.productId._id,
                                quantity: item.quantity,
                                selectedMeasurement: item.selectedMeasurement
                            })),
                            totalAmount: formData.get('totalAmount'),
                            billingAddress: {
                                fullName: formData.get('billingAddress[fullName]'),
                                email: formData.get('billingAddress[email]'),
                                phone: formData.get('billingAddress[phone]'),
                                country: formData.get('billingAddress[country]'),
                                city: formData.get('billingAddress[city]'),
                                address: formData.get('billingAddress[address]'),
                                zipCode: formData.get('billingAddress[zipCode]')
                            },
                            deliveryAddress: {
                                fullName: formData.get('deliveryAddress[fullName]'),
                                email: formData.get('deliveryAddress[email]'),
                                phone: formData.get('deliveryAddress[phone]'),
                                country: formData.get('deliveryAddress[country]'),
                                city: formData.get('deliveryAddress[city]'),
                                address: formData.get('deliveryAddress[address]'),
                                zipCode: formData.get('deliveryAddress[zipCode]')
                            },
                            orderNotes: formData.get('orderNotes'),
                            paymentMethod: formData.get('paymentMethod')
                        };

                        console.log("Sending data to backend:", data);

                        try {
                            const response = await fetch("/checkoutApi", {
                                method: "POST",
                                headers: { "Content-Type": "application/json" },
                                body: JSON.stringify(data)
                            });
                            const result = await response.json();

                            if (result.success) {
                                if (result.payuData) {
                                    // Redirect to PayU payment page
                                    const payuUrl = "https://secure.payu.in/_payment"; // PayU test URL
                                    const form = document.createElement("form");
                                    form.method = "POST";
                                    form.action = payuUrl;

                                    for (const key in result.payuData) {
                                        const input = document.createElement("input");
                                        input.type = "hidden";
                                        input.name = key;
                                        input.value = result.payuData[key];
                                        form.appendChild(input);
                                    }

                                    document.body.appendChild(form);
                                    form.submit();
                                } else {
                                    window.location.href = "/orders";
                                }
                            } else {
                                Toastify({
                                    text: result.message || "Payment data not received. Please try again.",
                                    duration: 3000,
                                    gravity: "top",
                                    position: "right",
                                    backgroundColor: "#FF0000"
                                }).showToast();
                            }
                        } catch (error) {
                            console.error("Error during checkout:", error);
                            Toastify({
                                text: "An error occurred during checkout. Please try again.",
                                duration: 3000,
                                gravity: "top",
                                position: "right",
                                backgroundColor: "#FF0000"
                            }).showToast();
                        }
                    }
                </script>
