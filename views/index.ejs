                        <h1 class="section-title">All Products</h1>
                    </div>
                </div>
            </div>
            <div class="row ltn__tab-product-slider-one-active--- slick-arrow-1">
                <% allProducts.forEach(function(product) { 
                    var mainImage = (product.images && product.images.length > 0) ? product.images[0] : 'img/product-2/1.png';
                    var basePrice = typeof product.basePrice === 'number' ? product.basePrice : 0;
                    var baseOffer = typeof product.baseOfferPrice === 'number' ? product.baseOfferPrice : 0;
                    var isOutOfStock = (typeof product.baseStocks === 'number' ? product.baseStocks : 0) <= 0; 
                %>
                <div class="col-lg-3 col-md-4 col-sm-6 col-6">
                    <div class="ltn__product-item ltn__product-item-2 text-left" data-product-id="<%= product._id %>">
                        <div class="product-img">
                            <a href="/product/<%= product._id %>">
                                <img src="<%= mainImage %>" alt="<%= product.title %>">
                            </a>
                            <% if (product.baseOfferPrice && product.baseOfferPrice > 0) { %>
                                <div class="product-badge">
                                    <ul>
                                        <li class="sale-badge">Offer</li>
                                    </ul>
                                </div>
                            <% } %>
                            <% if (isOutOfStock) { %>
                                <div class="product-badge">
                                    <ul>
                                        <li class="sold-out-badge">Out of Stock</li>
                                    </ul>
                                </div>
                            <% } %>
                        </div>
                        <div class="product-info">
                            <h2 class="product-title">
                                <a href="/product/<%= product._id %>">
                                    <%= product.title %>
                                </a>
                            </h2>
                            <div class="product-price" id="product-price-<%= product._id %>">
                                <b>₹</b>
                                <span class="main-price">
                                    <%= (baseOffer > 0 && baseOffer < basePrice) ? 
                                        baseOffer.toLocaleString('en-IN', {minimumFractionDigits: 2, maximumFractionDigits: 2}) : 
                                        basePrice.toLocaleString('en-IN', {minimumFractionDigits: 2, maximumFractionDigits: 2}) %>
                                </span>
                                <% if (baseOffer > 0 && baseOffer < basePrice) { %>
                                    <del><b>₹</b>
                                        <%= basePrice.toLocaleString('en-IN', {minimumFractionDigits: 2, maximumFractionDigits: 2}) %>
                                    </del>
                                <% } %>
                            </div>
                            <button id="addToCart_<%= product._id %>"
                                class="btn-theme btn-add-to-cart" 
                                title="Add to Cart"
                                data-product-id="<%= product._id %>" 
                                <%= isOutOfStock ? 'disabled style="opacity: 0.5;"' : '' %>>
                                <i class="fas fa-shopping-cart"></i> Add to Cart
                            </button>
                            <div class="add-to-cart-feedback" id="cart-feedback-<%= product._id %>"
                                style="display:none;margin-top:5px;"></div>
                        </div>
                    </div>
                </div>
                <% }); %>
            </div>

            <script>
                // Cart logic without variants
                const cart = {
                    async addItem(productId, quantity = 1) {
                        try {
                            const productElement = document.querySelector(`[data-product-id="${productId}"]`)?.closest('.ltn__product-item') || 
                                                 document.querySelector(`#addToCart_${productId}`)?.closest('.ltn__product-item');
                            if (!productElement) {
                                console.error('Product element not found for ID:', productId);
                                this.showError('Product not found. Please refresh the page and try again.', null);
                                return false;
                            }
                            const feedbackEl = productElement.querySelector(`#cart-feedback-${productId}`);
                            const addToCartBtn = productElement.querySelector(`#addToCart_${productId}`);
                            if (addToCartBtn) {
                                addToCartBtn.innerHTML = '<i class="fas fa-spinner fa-spin"></i> Adding...';
                                addToCartBtn.disabled = true;
                            }
                            if (feedbackEl) {
                                feedbackEl.style.display = "none";
                                feedbackEl.textContent = "";
                            }
                            const response = await fetch('/add-to-cart', {
                                method: 'PUT',
                                headers: {
                                    'Content-Type': 'application/json',
                                },
                                body: JSON.stringify({
                                    productId,
                                    quantity: parseInt(quantity)
                                })
                            });
                            if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);
                            const data = await response.json();
                            if (addToCartBtn) {
                                addToCartBtn.innerHTML = '<i class="fas fa-shopping-cart"></i> Add to Cart';
                                addToCartBtn.disabled = false;
                            }
                            if (data.success) {
                                this.showSuccess('Product added to cart!', feedbackEl);
                                const itemCount = data.isGuest ? this.getGuestCart().length : (data.cart?.length || 0);
                                this.updateCartCount(itemCount);
                                if (data.isGuest && data.cartItem) {
                                    this.addToGuestCart(data.cartItem);
                                }
                                return true;
                            } else {
                                this.showError(data.message || 'Failed to add to cart', feedbackEl);
                                return false;
                            }
                        } catch (error) {
                            console.error('Error adding to cart:', error);
                            const addToCartBtn = document.getElementById(`addToCart_${productId}`);
                            const feedbackEl = document.getElementById(`cart-feedback-${productId}`);
                            if (addToCartBtn) {
                                addToCartBtn.innerHTML = '<i class="fas fa-shopping-cart"></i> Add to Cart';
                                addToCartBtn.disabled = false;
                            }
                            this.showError('Error adding to cart. Please try again.', feedbackEl);
                            return false;
                        }
                    },
                    // Guest cart helpers
                    getGuestCart() {
                        return JSON.parse(localStorage.getItem('guestCart') || '[]');
                    },
                    addToGuestCart(item) {
                        const guestCart = this.getGuestCart();
                        // Only use productId as key since no variants
                        const cartItemKey = item.productId;
                        item.cartItemKey = cartItemKey;
                        const existingIndex = guestCart.findIndex(i => i.cartItemKey === cartItemKey);
                        if (existingIndex >= 0) {
                            guestCart[existingIndex].quantity += item.quantity;
                        } else {
                            guestCart.push(item);
                        }
                        localStorage.setItem('guestCart', JSON.stringify(guestCart));
                        this.updateCartCount(guestCart.length);
                    },
                    updateCartCount(count) {
                        document.querySelectorAll('.cart-count').forEach(el => {
                            el.textContent = count;
                            el.style.display = count > 0 ? 'inline-block' : 'none';
                        });
                    },
                    showSuccess(message, feedbackEl) {
                        if (feedbackEl) {
                            feedbackEl.textContent = message;
                            feedbackEl.style.display = "block";
                            feedbackEl.style.color = "#4CAF50";
                        }
                        if (typeof Toastify === 'function') {
                            Toastify({
                                text: message,
                                duration: 3000,
                                close: true,
                                gravity: "top",
                                position: "right",
                                backgroundColor: "#4CAF50",
                            }).showToast();
                        }
                    },
                    showError(message, feedbackEl) {
                        if (feedbackEl) {
                            feedbackEl.textContent = message;
                            feedbackEl.style.display = "block";
                            feedbackEl.style.color = "#f44336";
                        }
                        if (typeof Toastify === 'function') {
                            Toastify({
                                text: message,
                                duration: 3000,
                                close: true,
                                gravity: "top",
                                position: "right",
                                backgroundColor: "#f44336",
                            }).showToast();
                        }
                    }
                };

                function handleAddToCart(event) {
                    event.preventDefault();
                    event.stopPropagation();
                    const button = event.currentTarget;
                    if (button.disabled) return;
                    const productElement = button.closest('.ltn__product-item');
                    if (!productElement) {
                        console.error('Could not find product item container');
                        return;
                    }
                    const productId = button.getAttribute('data-product-id');
                    if (!productId) {
                        console.error('No product ID found on button');
                        return;
                    }
                    cart.addItem(productId, 1);
                }

                // Initialize on DOM load
                document.addEventListener('DOMContentLoaded', function() {
                    // Initialize cart count
                    <% if (typeof user !== "undefined" && user && typeof user.cartCount !== "undefined") { %>
                        cart.updateCartCount(<%= user.cartCount %>);
                    <% } else { %>
                        cart.updateCartCount(cart.getGuestCart().length);
                    <% } %>
                    // Attach event listeners to all add-to-cart buttons
                    document.querySelectorAll('button.btn-add-to-cart').forEach(button => {
                        button.removeEventListener('click', handleAddToCart); // Prevent duplicates
                        button.addEventListener('click', handleAddToCart);
                    });
                });
            </script>


