    <!-- PRODUCT DETAILS AREA START -->
    <div class="ltn__product-area ltn__product-gutter mb-120">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="ltn__shop-options">
                        <ul>
                            <li>
                                <div class="ltn__grid-list-tab-menu ">
                                    <div class="nav">
                                        <a class="active show" data-bs-toggle="tab" href="#liton_product_grid"><i class="fas fa-th-large"></i></a>
                                        <a data-bs-toggle="tab" href="#liton_product_list"><i class="fas fa-list"></i></a>
                                    </div>
                                </div>
                            </li>
                            <li>
                                <!-- Category Filter -->
                                <div class="short-by text-center">
                                    <form method="GET" id="categoryForm" style="display:inline;">
                                        <select class="nice-select" name="category" onchange="document.getElementById('categoryForm').submit();" style="min-width:120px;">
                                            <option value="">All Categories</option>
                                            <option value="General" <%= selectedCategory === 'General' ? 'selected' : '' %>>General</option>
                                            <option value="Healthcare" <%= selectedCategory === 'Healthcare' ? 'selected' : '' %>>Health Care</option>
                                        </select>
                                        <% if (sort) { %>
                                            <input type="hidden" name="sort" value="<%= sort %>">
                                        <% } %>
                                        <% if (page) { %>
                                            <input type="hidden" name="page" value="<%= page %>">
                                        <% } %>
                                    </form>
                                </div>
                            </li>
                            <li>
                               <div class="short-by text-center">
                                    <form method="GET" id="sortForm">
                                        <% if (selectedCategory) { %>
                                            <input type="hidden" name="category" value="<%= selectedCategory %>">
                                        <% } %>
                                        <select class="nice-select" name="sort" onchange="document.getElementById('sortForm').submit();">
                                            <option value="">Default sorting</option>
                                            <option value="popularity" <%= sort === 'popularity' ? 'selected' : '' %>>Sort by popularity</option>
                                            <option value="new" <%= sort === 'new' ? 'selected' : '' %>>Sort by new arrivals</option>
                                            <option value="price-asc" <%= sort === 'price-asc' ? 'selected' : '' %>>Sort by price: low to high</option>
                                            <option value="price-desc" <%= sort === 'price-desc' ? 'selected' : '' %>>Sort by price: high to low</option>
                                        </select>
                                    </form>
                                </div> 
                            </li>
                            <li>
                               <div class="showing-product-number text-right">
                                    <span>
                                        Showing 
                                        <%= products && products.length > 0 ? ((page - 1) * perPage + 1) : 0 %>
                                        -
                                        <%= products && products.length > 0 ? ((page - 1) * perPage + products.length) : 0 %>
                                        of
                                        <%= totalProducts || 0 %>
                                        results
                                    </span>
                                </div> 
                            </li>
                        </ul>
                    </div>
                    <div class="tab-content">
                        <!-- Grid View -->
                        <div class="tab-pane fade active show" id="liton_product_grid">
                            <div class="ltn__product-tab-content-inner ltn__product-grid-view">
                                <div class="row ltn__tab-product-slider-one-active--- slick-arrow-1">
                                    <% if (products && products.length > 0) { %>
                                        <% products.forEach(function(product) { 
                                            var mainImage = (product.images && product.images.length > 0) ? product.images[0] : '/img/product/default.png';
                                            var basePrice = typeof product.basePrice === 'number' ? product.basePrice : 0;
                                            var baseOffer = typeof product.baseOfferPrice === 'number' ? product.baseOfferPrice : 0;
                                            var price = (baseOffer > 0 && baseOffer < basePrice) ? baseOffer : basePrice;
                                            var oldPrice = (baseOffer > 0 && baseOffer < basePrice) ? basePrice : null;
                                            var isOutOfStock = (typeof product.baseStocks === 'number' ? product.baseStocks : 0) <= 0;
                                            var hasMeasurements = product.measurements && product.measurements.length > 0;
                                            var hasColors = product.colorVariants && product.colorVariants.length > 0;
                                        %>
                                            <div class="col-xl-3 col-lg-4 col-sm-6 col-6 h-100">
                                                <div class="ltn__product-item ltn__product-item-3 text-center h-100">
                                                    <div class="product-img">
                                                        <a href="/product/<%= product._id %>">
                                                            <img class="Allproduct-image" src="<%= mainImage %>" alt="<%= product.title %>">
                                                        </a>
                                                        <% if (product.toggled === false) { %>
                                                        <div class="product-badge">
                                                            <ul>
                                                                <li class="sale-badge">Unavailable</li>
                                                            </ul>
                                                        </div>
                                                        <% } %>
                                                        <% if (isOutOfStock) { %>
                                                        <div class="product-badge">
                                                            <ul>
                                                                <li class="sold-out-badge">Out of Stock</li>
                                                            </ul>
                                                        </div>
                                                        <% } %>
                                                    </div>
                                                    <div class="product-info h-100">
                                                        <h2 class="product-title">
                                                            <a href="/product/<%= product._id %>"><%= product.title %></a>
                                                        </h2>
                                                        <% if (hasMeasurements || hasColors) { %>
                                                        <form class="variant-form w-100"
                                                            data-product-id="<%= product._id.toString() %>"
                                                            data-base-price="<%= basePrice %>"
                                                            data-base-offer="<%= baseOffer %>">
                                                            <% 
                                                                var hasMeasurementOptions = hasMeasurements &&
                                                                    Array.isArray(product.measurements) &&
                                                                    product.measurements.length > 0 &&
                                                                    product.measurements.some(function(m) { return m && m.measurement; });
                                                                var hasColorOptions = hasColors &&
                                                                    Array.isArray(product.colorVariants) &&
                                                                    product.colorVariants.length > 0 &&
                                                                    product.colorVariants.some(function(c) { return c && c.colorName; });
                                                            %>
                                                            <% if (hasMeasurementOptions || hasColorOptions) { %>
                                                            <div class="variant-selectors-row d-flex flex-wrap gap-2 mb-2 w-100">
                                                                <% if (hasMeasurementOptions) { %>
                                                                <div class="variant-selector-col flex-grow-1 mb-2 mb-md-0" style="min-width: 120px;">
                                                                    <select class="variant-select w-100"
                                                                        data-type="measurement"
                                                                        data-product-id="<%= product._id %>"
                                                                        onchange="window.updatePriceOnSelect && updatePriceOnSelect('<%= product._id %>')"
                                                                        style="padding: 6px 10px; border-radius: 6px; border: 1px solid #ddd; background: #fafbfc;">
                                                                        <option value="">Select Size</option>
                                                                        <% product.measurements.forEach(function(m, mi) { %>
                                                                            <option value="<%= mi %>"
                                                                                data-price="<%= m.price %>"
                                                                                data-offer="<%= m.offerPrice %>">
                                                                                <%= m.measurement %>
                                                                            </option>
                                                                        <% }) %>
                                                                    </select>
                                                                </div>
                                                                <% } %>
                                                                <% if (hasColorOptions) { %>
                                                                <div class="variant-selector-col flex-grow-1 mb-2 mb-md-0" style="min-width: 120px;">
                                                                    <select class="variant-select w-100"
                                                                        data-type="color"
                                                                        data-product-id="<%= product._id %>"
                                                                        onchange="window.updatePriceOnSelect && updatePriceOnSelect('<%= product._id %>')"
                                                                        style="padding: 6px 10px; border-radius: 6px; border: 1px solid #ddd; background: #fafbfc;">
                                                                        <option value="">Select Color</option>
                                                                        <% product.colorVariants.forEach(function(c, ci) { %>
                                                                            <option value="<%= ci %>"
                                                                                data-price="<%= c.price %>"
                                                                                data-offer="<%= c.offerPrice %>">
                                                                                <%= c.colorName %>
                                                                            </option>
                                                                        <% }) %>
                                                                    </select>
                                                                    <div class="color-chips mt-1">
                                                                        <% product.colorVariants.forEach(function(c, ci) { if (c.colorHex) { %>
                                                                            <span class="color-chip" title="<%= c.colorName %>" style="background:<%= c.colorHex %>"></span>
                                                                        <% } }) %>
                                                                    </div>
                                                                </div>
                                                                <% } %>
                                                            </div>
                                                            <% } %>
                                                        </form>
                                                        <% } %>
                                                        <div class="product-price" id="product-price-<%= product._id %>">
                                                            <span>₹<%= price ? price.toFixed(2) : '0.00' %></span>
                                                            <% if (oldPrice) { %>
                                                                <del>₹<%= oldPrice.toFixed(2) %></del>
                                                            <% } %>
                                                        </div>
                                                        <button id="addToCart_<%= product._id %>"
                                                            class="btn-theme btn-add-to-cart" title="Add to Cart"
                                                            data-product-id="<%= product._id %>" <%= isOutOfStock ? 'disabled style="opacity: 0.5;"' : '' %>>
                                                            <i class="fas fa-shopping-cart"></i> Add to Cart
                                                        </button>
                                                        <div class="add-to-cart-feedback"
                                                            id="cart-feedback-<%= product._id %>"
                                                            style="display:none;margin-top:5px;"></div>
                                                    </div>
                                                </div>
                                            </div>
                                        <% }); %>
                                    <% } else { %>
                                        <div class="col-12">
                                            <p>No products found.</p>
                                        </div>
                                    <% } %>
                                </div>
                            </div>
                        </div>
                        <!-- List View -->
                        <div class="tab-pane fade" id="liton_product_list">
                            <div class="ltn__product-tab-content-inner ltn__product-list-view">
                                <div class="row">
                                    <% if (products && products.length > 0) { %>
                                        <% products.forEach(function(product) { 
                                            var mainImage = (product.images && product.images.length > 0) ? product.images[0] : '/img/product/default.png';
                                            var basePrice = typeof product.basePrice === 'number' ? product.basePrice : 0;
                                            var baseOffer = typeof product.baseOfferPrice === 'number' ? product.baseOfferPrice : 0;
                                            var price = (baseOffer > 0 && baseOffer < basePrice) ? baseOffer : basePrice;
                                            var oldPrice = (baseOffer > 0 && baseOffer < basePrice) ? basePrice : null;
                                            var isOutOfStock = (typeof product.baseStocks === 'number' ? product.baseStocks : 0) <= 0;
                                            var hasMeasurements = product.measurements && product.measurements.length > 0;
                                            var hasColors = product.colorVariants && product.colorVariants.length > 0;
                                            var description = '';
                                            if (product.productDescription && Array.isArray(product.productDescription) && product.productDescription.length > 0) {
                                                description = product.productDescription.map(function(p) { return p.point; }).join(' ');
                                            }
                                        %>
                                            <div class="col-lg-12">
                                                <div class="ltn__product-item ltn__product-item-3">
                                                    <div class="product-img">
                                                        <a href="/product/<%= product._id %>">
                                                            <img src="<%= mainImage %>" alt="<%= product.title %>">
                                                        </a>
                                                        <% if (product.toggled === false) { %>
                                                        <div class="product-badge">
                                                            <ul>
                                                                <li class="sale-badge">Unavailable</li>
                                                            </ul>
                                                        </div>
                                                        <% } %>
                                                        <% if (isOutOfStock) { %>
                                                        <div class="product-badge">
                                                            <ul>
                                                                <li class="sold-out-badge">Out of Stock</li>
                                                            </ul>
                                                        </div>
                                                        <% } %>
                                                    </div>
                                                    <div class="product-info">
                                                        <h2 class="product-title">
                                                            <a href="/product/<%= product._id %>"><%= product.title %></a>
                                                        </h2>
                                                        <% if (hasMeasurements || hasColors) { %>
                                                        <form class="variant-form w-100"
                                                            data-product-id="<%= product._id.toString() %>"
                                                            data-base-price="<%= basePrice %>"
                                                            data-base-offer="<%= baseOffer %>">
                                                            <% 
                                                                var hasMeasurementOptions = hasMeasurements &&
                                                                    Array.isArray(product.measurements) &&
                                                                    product.measurements.length > 0 &&
                                                                    product.measurements.some(function(m) { return m && m.measurement; });
                                                                var hasColorOptions = hasColors &&
                                                                    Array.isArray(product.colorVariants) &&
                                                                    product.colorVariants.length > 0 &&
                                                                    product.colorVariants.some(function(c) { return c && c.colorName; });
                                                            %>
                                                            <% if (hasMeasurementOptions || hasColorOptions) { %>
                                                            <div class="variant-selectors-row d-flex flex-wrap gap-2 mb-2 w-100">
                                                                <% if (hasMeasurementOptions) { %>
                                                                <div class="variant-selector-col flex-grow-1 mb-2 mb-md-0" style="min-width: 120px;">
                                                                    <select class="variant-select w-100"
                                                                        data-type="measurement"
                                                                        data-product-id="<%= product._id %>"
                                                                        onchange="window.updatePriceOnSelect && updatePriceOnSelect('<%= product._id %>')"
                                                                        style="padding: 6px 10px; border-radius: 6px; border: 1px solid #ddd; background: #fafbfc;">
                                                                        <option value="">Select Size</option>
                                                                        <% product.measurements.forEach(function(m, mi) { %>
                                                                            <option value="<%= mi %>"
                                                                                data-price="<%= m.price %>"
                                                                                data-offer="<%= m.offerPrice %>">
                                                                                <%= m.measurement %>
                                                                            </option>
                                                                        <% }) %>
                                                                    </select>
                                                                </div>
                                                                <% } %>
                                                                <% if (hasColorOptions) { %>
                                                                <div class="variant-selector-col flex-grow-1 mb-2 mb-md-0" style="min-width: 120px;">
                                                                    <select class="variant-select w-100"
                                                                        data-type="color"
                                                                        data-product-id="<%= product._id %>"
                                                                        onchange="window.updatePriceOnSelect && updatePriceOnSelect('<%= product._id %>')"
                                                                        style="padding: 6px 10px; border-radius: 6px; border: 1px solid #ddd; background: #fafbfc;">
                                                                        <option value="">Select Color</option>
                                                                        <% product.colorVariants.forEach(function(c, ci) { %>
                                                                            <option value="<%= ci %>"
                                                                                data-price="<%= c.price %>"
                                                                                data-offer="<%= c.offerPrice %>">
                                                                                <%= c.colorName %>
                                                                            </option>
                                                                        <% }) %>
                                                                    </select>
                                                                    <div class="color-chips mt-1">
                                                                        <% product.colorVariants.forEach(function(c, ci) { if (c.colorHex) { %>
                                                                            <span class="color-chip" title="<%= c.colorName %>" style="background:<%= c.colorHex %>"></span>
                                                                        <% } }) %>
                                                                    </div>
                                                                </div>
                                                                <% } %>
                                                            </div>
                                                            <% } %>
                                                        </form>
                                                        <% } %>
                                                        <div class="product-price" id="product-price-<%= product._id %>">
                                                            <span>₹<%= price ? price.toFixed(2) : '0.00' %></span>
                                                            <% if (oldPrice) { %>
                                                                <del>₹<%= oldPrice.toFixed(2) %></del>
                                                            <% } %>
                                                        </div>
                                                        <div class="product-brief">
                                                            <p><%= description ? description : 'No description available.' %></p>
                                                        </div>
                                                        <button id="addToCart_<%= product._id %>"
                                                            class="btn-theme btn-add-to-cart" title="Add to Cart"
                                                            data-product-id="<%= product._id %>" <%= isOutOfStock ? 'disabled style="opacity: 0.5;"' : '' %>>
                                                            <i class="fas fa-shopping-cart"></i> Add to Cart
                                                        </button>
                                                        <div class="add-to-cart-feedback"
                                                            id="cart-feedback-<%= product._id %>"
                                                            style="display:none;margin-top:5px;"></div>
                                                    </div>
                                                </div>
                                            </div>
                                        <% }); %>
