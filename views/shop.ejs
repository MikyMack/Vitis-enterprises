                                    </li>
                                <% } %>
                                <% if (page < totalPages) { %>
                                    <li>
                                        <a href="<%= buildQuery(page + 1) %>">
                                            <i class="fas fa-angle-double-right"></i>
                                        </a>
                                    </li>
                                <% } %>
                            </ul>
                        </div>
                    </div>
