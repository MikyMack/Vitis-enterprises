                                <p><a href="tel:+917736921309">+91 7736921309</a></p>
                            </div>
                        </div>
                        <!-- header-search-2 -->
                        <div class="header-search-2">
                            <form method="get" action="/shop">
                                <input 
                                    type="text" 
                                    name="search" 
                                    value="<%= search || '' %>" 
                                    id="typewriter-search"
                                    placeholder="Search by Products names"
                                    autocomplete="off"
                                />
                                <button type="submit">
                                    <span><i class="icon-search"></i></span>
                                </button>
                            </form>
                            <script>
                                (function() {
                                    const placeholders = [
                                        "Search by Products names",
                                        "General categories",
                                        "Healthcare categories"
                                    ];
                                    let current = 0;
                                    let charIndex = 0;
                                    let typing = true;
                                    const input = document.getElementById('typewriter-search');
                                    let interval;

                                    function typeWriter() {
                                        if (!input) return;
                                        if (typing) {
                                            if (charIndex < placeholders[current].length) {
                                                input.setAttribute('placeholder', placeholders[current].substring(0, charIndex + 1));
                                                charIndex++;
                                            } else {
                                                typing = false;
                                                setTimeout(typeWriter, 1200);
                                                return;
                                            }
                                        } else {
                                            if (charIndex > 0) {
                                                input.setAttribute('placeholder', placeholders[current].substring(0, charIndex - 1));
                                                charIndex--;
                                            } else {
                                                typing = true;
                                                current = (current + 1) % placeholders.length;
                                            }
                                        }
                                        interval = setTimeout(typeWriter, typing ? 70 : 30);
                                    }


                                    if (input && !input.value) {
                                        typeWriter();
    
                                        input.addEventListener('focus', function() {
                                            clearTimeout(interval);
                                            input.setAttribute('placeholder', '');
                                        });
                                        input.addEventListener('blur', function() {
                                            charIndex = 0;
                                            typing = true;
                                            typeWriter();
                                        });
                                    }
                                })();
                            </script>
                        </div>
                    </div>
                </div>
                <div class="col">
                    <!-- header-options -->
                    <div class="ltn__header-options">
                        <ul>

                            <li class="d-none--- ">
                                <!-- header-search-1 -->
                                <div class="header-search-wrap d-block d-xl-none">
                                    <div class="header-search-1">
                                        <div class="search-icon">
                                            <i class="icon-search  for-search-show"></i>
                                            <i class="icon-cancel  for-search-close"></i>
                                        </div>
                                    </div>
                                    <div class="header-search-1-form">
                                        <form method="get" action="/shop">
                                            <input type="text" name="search" value="<%= search || '' %>" placeholder="Search here..." />
                                            <button type="submit">
                                                <span><i class="icon-search"></i></span>
                                            </button>
                                        </form> 
                                    </div>
                                </div>
                            </li>
                            <li class="d-none---">
                                <!-- user-menu -->
                                <div class="ltn__drop-menu user-menu">
                                    <ul>
                                        <li>
                                            <a href="#"><i class="icon-user"></i></a>
                                            <% if (user) { %>
                                                <ul>
                                                    <li><a href="/orders">Orders</a></li>
                                                    <li><a href="/cart">Cart</a></li>
                                                    <li><a href="/api/auth/user-logout">Logout</a></li>
                                                </ul>
                                                <% } else { %>
                                                    <ul>
                                                        <li><a href="/user-login">Sign in</a></li>
                                                        <li><a href="/user-register">Register</a></li>
                                                    </ul>
                                                    <% } %>
                                        </li>
                                    </ul>
                                </div>
                            </li>
                            <li>
                                <!-- mini-cart 2 -->
                                <div class="mini-cart-icon mini-cart-icon-2">
                                    <a href="/cart">
                                        <span class="mini-cart-icon">
                                            <i class="icon-shopping-cart"></i>
                                            <sup>
                                                <% if (user) { %>
                                                    <%= typeof cartCount !=='undefined' ? cartCount : 0 %>
                                                        <% } else { %>
                                                            <script>
                                                                (function () {
                                                                    try {
                                                                        var guestCart = localStorage.getItem('guestCart');
                                                                        var count = 0;
                                                                        if (guestCart) {
                                                                            var items = JSON.parse(guestCart);
                                                                            if (Array.isArray(items)) {
                                                                                count = items.length;
                                                                            }
                                                                        }
                                                                        var sup = document.currentScript.parentElement;
                                                                        if (sup) sup.textContent = count;
                                                                    } catch (e) {
                                                                        var sup = document.currentScript.parentElement;
                                                                        if (sup) sup.textContent = 0;
                                                                    }
                                                                })();
                                                            </script>
                                                            <% } %>
                                            </sup>
                                        </span>
                                        <h6><span>Your Cart</span></h6>
                                    </a>
                                </div>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
