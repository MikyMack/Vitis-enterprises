                    <script>
                        function generateCartItemKey(item) {
                            const color = (item.selectedColor && typeof item.selectedColor.colorName !== "undefined")
                                ? item.selectedColor.colorName
                                : (typeof item.colorName !== "undefined" ? item.colorName : 'none');
                            const measurement = (item.selectedMeasurement && typeof item.selectedMeasurement.measurement !== "undefined")
                                ? item.selectedMeasurement.measurement
                                : (typeof item.measurement !== "undefined" ? item.measurement : 'none');
                            return `${item.productId}-${color}-${measurement}`;
                        }

                        window.updatePriceOnSelect = function(productId) {
                            const productItems = document.querySelectorAll('.ltn__product-item [data-product-id="' + productId + '"]');
                            productItems.forEach(function(el) {
                                let productElement = el.closest('.ltn__product-item');
                                if (!productElement) return;

                                const variantForm = productElement.querySelector('.variant-form');
                                if (!variantForm) return;

                                const basePrice = parseFloat(variantForm.getAttribute('data-base-price')) || 0;
                                const baseOffer = parseFloat(variantForm.getAttribute('data-base-offer')) || 0;

                                let finalPrice = basePrice;
                                let finalOffer = baseOffer;

                                const selects = productElement.querySelectorAll('.variant-select');

                                selects.forEach(select => {
                                    if (!select || typeof select.selectedIndex !== "number" || !select.options || select.options.length === 0) return;
                                    const selectedOption = select.options[select.selectedIndex];
                                    if (selectedOption && selectedOption.value !== '') {
                                        const priceAttr = selectedOption.getAttribute('data-price');
                                        const offerAttr = selectedOption.getAttribute('data-offer');
                                        const variantPrice = priceAttr !== null && priceAttr !== undefined && priceAttr !== "" ? parseFloat(priceAttr) : NaN;
                                        const variantOffer = offerAttr !== null && offerAttr !== undefined && offerAttr !== "" ? parseFloat(offerAttr) : NaN;

                                        if (!isNaN(variantPrice)) finalPrice = variantPrice;
                                        if (!isNaN(variantOffer)) finalOffer = variantOffer;
                                    }
                                });

                                const displayPrice = (finalOffer > 0 && finalOffer < finalPrice) ? finalOffer : finalPrice;
                                const priceEl = productElement.querySelector('#product-price-' + productId);

                                if (priceEl) {
                                    const formatOptions = { minimumFractionDigits: 2, maximumFractionDigits: 2 };
                                    let mainPriceSpan = null;
                                    const spans = priceEl.querySelectorAll('span');
                                    spans.forEach(span => {
                                        if (!span.closest('del')) mainPriceSpan = span;
                                    });

                                    if (mainPriceSpan) {
                                        mainPriceSpan.textContent = '₹' + displayPrice.toLocaleString('en-IN', formatOptions);
                                    }

                                    let delTag = priceEl.querySelector('del');
                                    if (finalOffer > 0 && finalOffer < finalPrice) {
                                        if (!delTag) {
                                            delTag = document.createElement('del');
                                            delTag.innerHTML = '₹' + finalPrice.toLocaleString('en-IN', formatOptions);
                                            priceEl.appendChild(delTag);
                                        } else {
                                            delTag.innerHTML = '₹' + finalPrice.toLocaleString('en-IN', formatOptions);
                                        }
                                    } else if (delTag) {
                                        delTag.remove();
                                    }
                                }
                            });
                        }

                        const cart = {
                            async addItem(productId, colorIndex = null, measurementIndex = null, quantity = 1) {
                                try {
                               
                                    let productElement = null;
                                  
                                    let el = document.querySelector(`#addToCart_${productId}`);
                                    if (el) productElement = el.closest('.ltn__product-item');
                             
                                    if (!productElement) {
                                        el = document.querySelector('.variant-form[data-product-id="' + productId + '"]');
                                        if (el) productElement = el.closest('.ltn__product-item');
                                    }
                              
                                    if (!productElement) {
                                        el = document.querySelector('[data-product-id="' + productId + '"]');
                                        if (el) productElement = el.closest('.ltn__product-item');
                                    }

                                    if (!productElement) {
                                        console.error('Product element not found for ID:', productId);
                                        this.showError('Product not found. Please refresh the page and try again.', null);
                                        return false;
                                    }

                                    const feedbackEl = productElement.querySelector(`#cart-feedback-${productId}`);
                                    const variantForm = productElement.querySelector('.variant-form');
                                    let colorSelect = variantForm ? variantForm.querySelector('select[data-type="color"]') : null;
                                    let measurementSelect = variantForm ? variantForm.querySelector('select[data-type="measurement"]') : null;

                                    if (colorSelect && colorSelect.options.length > 1) {
                                        if (!colorSelect.value) {
                                            this.showError('Please select a color.', feedbackEl);
                                            return false;
                                        }
                                        colorIndex = colorSelect.value;
                                    }

                                    if (measurementSelect && measurementSelect.options.length > 1) {
                                        if (!measurementSelect.value) {
                                            this.showError('Please select a size.', feedbackEl);
                                            return false;
                                        }
                                        measurementIndex = measurementSelect.value;
                                    }

                                    const addToCartBtn = productElement.querySelector(`#addToCart_${productId}`);
                                    if (addToCartBtn) {
                                        addToCartBtn.innerHTML = '<i class="fas fa-spinner fa-spin"></i> Adding...';
                                        addToCartBtn.disabled = true;
                                    }

                                    if (feedbackEl) {
                                        feedbackEl.style.display = "none";
                                        feedbackEl.textContent = "";
                                    }

                                    const response = await fetch('/add-to-cart', {
                                        method: 'PUT',
                                        headers: {
                                            'Content-Type': 'application/json',
                                        },
                                        body: JSON.stringify({
                                            productId,
                                            colorIndex: colorIndex !== null && colorIndex !== "" ? parseInt(colorIndex) : null,
                                            measurementIndex: measurementIndex !== null && measurementIndex !== "" ? parseInt(measurementIndex) : null,
                                            quantity: parseInt(quantity)
                                        })
                                    });

                                    if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);

                                    const data = await response.json();

                                    if (addToCartBtn) {
                                        addToCartBtn.innerHTML = '<i class="fas fa-shopping-cart"></i> Add to Cart';
                                        addToCartBtn.disabled = false;
                                    }

                                    if (data.success) {
                                        this.showSuccess('Product added to cart!', feedbackEl);
                                        const itemCount = data.isGuest ? this.getGuestCart().length : (data.cart && Array.isArray(data.cart) ? data.cart.length : 0);
                                        this.updateCartCount(itemCount);

                                        if (data.isGuest && data.cartItem) {
                                            this.addToGuestCart(data.cartItem);
                                        }
                                        return true;
                                    } else {
                                        this.showError(data.message || 'Failed to add to cart', feedbackEl);
                                        return false;
                                    }
                                } catch (error) {
                                    console.error('Error adding to cart:', error);
                                    const addToCartBtn = document.getElementById(`addToCart_${productId}`);
                                    const feedbackEl = document.getElementById(`cart-feedback-${productId}`);
                                    if (addToCartBtn) {
                                        addToCartBtn.innerHTML = '<i class="fas fa-shopping-cart"></i> Add to Cart';
                                        addToCartBtn.disabled = false;
                                    }
                                    this.showError('Error adding to cart. Please try again.', feedbackEl);
                                    return false;
                                }
                            },

                            getGuestCart() {
                                try {
                                    const cart = JSON.parse(localStorage.getItem('guestCart') || '[]');
                                    return Array.isArray(cart) ? cart : [];
                                } catch (e) {
                                    return [];
                                }
                            },

                            addToGuestCart(item) {
                                const guestCart = this.getGuestCart();
                                const cartItemKey = generateCartItemKey(item);
                                item.cartItemKey = cartItemKey;

                                const existingIndex = guestCart.findIndex(i => i.cartItemKey === cartItemKey);

                                if (existingIndex >= 0) {
                                    guestCart[existingIndex].quantity += item.quantity;
                                } else {
                                    guestCart.push(item);
                                }

                                localStorage.setItem('guestCart', JSON.stringify(guestCart));
                                this.updateCartCount(guestCart.length);
                            },

                            updateCartCount(count) {
                                document.querySelectorAll('.cart-count').forEach(el => {
                                    if (typeof count === "undefined" || count === null) count = 0;
                                    el.textContent = count;
                                    el.style.display = count > 0 ? 'inline-block' : 'none';
                                });
                            },

                            showSuccess(message, feedbackEl) {
                                if (feedbackEl) {
                                    feedbackEl.textContent = message;
                                    feedbackEl.style.display = "block";
                                    feedbackEl.style.color = "#4CAF50";
                                }
                                if (typeof Toastify === 'function') {
                                    Toastify({
                                        text: message,
                                        duration: 3000,
                                        close: true,
                                        gravity: "top",
                                        position: "right",
                                        backgroundColor: "#4CAF50",
                                    }).showToast();
                                }
                            },

                            showError(message, feedbackEl) {
                                if (feedbackEl) {
                                    feedbackEl.textContent = message;
                                    feedbackEl.style.display = "block";
                                    feedbackEl.style.color = "#f44336";
                                }
                                if (typeof Toastify === 'function') {
                                    Toastify({
                                        text: message,
                                        duration: 3000,
                                        close: true,
                                        gravity: "top",
                                        position: "right",
                                        backgroundColor: "#f44336",
                                    }).showToast();
                                }
                            }
                        };

                        function handleAddToCart(event) {
                            event.preventDefault();
                            event.stopPropagation();

                            const button = event.currentTarget;
                            if (button.disabled) return;

                            const productElement = button.closest('.ltn__product-item');
                            if (!productElement) {
                                console.error('Could not find product item container');
                                return;
                            }

                            const productId = button.getAttribute('data-product-id');
                            if (!productId) {
                                console.error('No product ID found on button');
                                return;
                            }

                            const variantForm = productElement.querySelector('.variant-form');
                            let colorIndex = null, measurementIndex = null;

                            if (variantForm) {
                                const colorSelect = variantForm.querySelector('select[data-type="color"]');
                                const measurementSelect = variantForm.querySelector('select[data-type="measurement"]');

                                colorIndex = colorSelect ? colorSelect.value : null;
                                measurementIndex = measurementSelect ? measurementSelect.value : null;
                            }

                            cart.addItem(productId, colorIndex, measurementIndex, 1);
                        }

                        document.addEventListener('DOMContentLoaded', function() {
                            <% if (typeof user !== "undefined" && user && typeof user.cartCount !== "undefined") { %>
                                cart.updateCartCount(<%= user.cartCount %>);
                            <% } else { %>
                                cart.updateCartCount(cart.getGuestCart().length);
                            <% } %>

                       
                            document.querySelectorAll('button.btn-add-to-cart').forEach(button => {
                                button.removeEventListener('click', handleAddToCart); 
                                button.addEventListener('click', handleAddToCart);
                            });

                  
                            document.querySelectorAll('.variant-select').forEach(function(select) {
                                select.removeEventListener('change', variantSelectChangeHandler);
                                select.addEventListener('change', variantSelectChangeHandler);
                            });

                            if (typeof jQuery !== 'undefined' && jQuery.fn.niceSelect) {
                                jQuery('.variant-select').each(function() {
                                    jQuery(this).data('nice-select-initialized', true);
                                });
                            }
                        });

              
                        function variantSelectChangeHandler(e) {
                            const select = e.currentTarget;
                            const productId = select.getAttribute('data-product-id');
                            if (productId) {
                                window.updatePriceOnSelect(productId);
                            }
                        }
                    </script>
